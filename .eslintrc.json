{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-underscore-dangle": 0,
    "camelcase": 0,
    "no-console": 0,
    "no-shadow": 0,
    "func-names": 0,
    "no-param-reassign": ["error", {
      "props": false
    }]
  }
}
